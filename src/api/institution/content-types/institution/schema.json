{
  "kind": "collectionType",
  "collectionName": "institutions",
  "info": {
    "singularName": "institution",
    "pluralName": "institutions",
    "displayName": "Institution",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "adress": {
      "type": "component",
      "repeatable": true,
      "component": "adress.address"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Hospital",
        "Laboratory",
        "other"
      ],
      "default": "Hospital",
      "required": true
    },
    "contact": {
      "type": "component",
      "repeatable": false,
      "component": "adress.contact"
    },
    "doctors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::doctor.doctor",
      "mappedBy": "institutions"
    },
    "specialisations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::specialisation.specialisation",
      "inversedBy": "institutions"
    },
    "promotions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::promotion.promotion",
      "mappedBy": "institution"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "institution"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "licences": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "institution"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
